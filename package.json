{
  "name": "bomb-buster",
  "version": "0.0.1",
  "description": "A Bomb Buster with AI Project",
  "productName": "Bomb Buster",
  "author": "Julien Reichel <julien.reichel@bluewin.ch>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "lint:fix": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\" --fix",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest --reporter=verbose",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:coverage:open": "vitest run --coverage && open coverage/index.html",
    "test-ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "test:browser": "vitest",
    "test:mutation": "stryker run",
    "test:mutation:critical": "stryker run --mutate 'src/composables/models/**/*.js,src/composables/managers/**/*.js'",
    "test:quality": "./scripts/test-quality-check.sh",
    "test:quality:advanced": "node scripts/advanced-test-analysis.mjs",
    "quality:check": "npm run lint && npm run format:check && npm run test:coverage && npm run test:quality",
    "quality:local": "./scripts/quality-check.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.3.0",
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/javascript-mutator": "^4.0.0",
    "@stryker-mutator/vitest-runner": "^9.1.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "playwright": "^1.55.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^3.2.4",
    "vitest-browser-vue": "^1.1.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
